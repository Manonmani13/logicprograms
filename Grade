
class Grade  {
    public static void main(String[] args) {
    String d="A";
    switch(d){
        case "A":
            System.out.println("student has got mark above 90 ");
            break;
        case "B":
            System.out.println("student has got mark above 80 ");
            break;
         case "C":
            System.out.println("student has got mark above 70 ");
            break;
         case "D":
            System.out.println("student has got mark above 60 ");
            break;
         case "E":
            System.out.println("student has got mark above 50 ");
            break;
    }

      }
}
