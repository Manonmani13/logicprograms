package com.proretention.scrm.communities.services;

public class Test {
    public static void main(String[] args) {
        int k=177;
       if(k>177)
       {
           System.out.println("your are tall");
       }
       else  if(k>=155 && k<=177)
           System.out.println("your middle ");
       else
           System.out.println("short");


    }
}
