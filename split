import java.util.*;

public class Main {
    public static void main(String[] args) {
      System.out.println("Hello, World!");
      String s="A|B|C|D|E";
     
      System.out.println(s.replace("|"," "));
      String val[]= s.replace("|"," ").split(" ");
      int j=4;
      for(int i=0;i<val.length;i++)
      {
        if(i==j)
        System.out.println(val[i]);
      }
  }
}
