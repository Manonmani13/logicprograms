
public class Test {
    public static void main(String[] args) {
        int n=10,sum=0;
        for(int i=1;i<n;i++)
        {
            if(n%i==0)
            sum+=i;
        }
        if(n==sum)
            System.out.println("Amstrong number");
        else
            System.out.println("not amstrong");
    }
}

