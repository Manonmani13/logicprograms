package com.proretention.scrm.communities.services;

public class Test {
    public static void main(String[] args) {
        String s="MaNo";
        int k=0;
        for(int i=0;i<s.length();i++)
        {
             k=(int)(s.charAt(i));
            if(k>=97&&k<=122)
            {
                System.out.println(s.charAt(i)+" lowercase letter");
            }
            else{
                System.out.println(s.charAt(i)+" upper case letter");

            }
        }
    }
}

